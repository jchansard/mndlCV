{{timeline-component icon="certificate"}}
<h2>{{title}}</h2>
{{#each sections as |section|}}
	<ul class="list-inline">
		<li class="emphasize italic text-capitalize">{{section.name}}</li>
		<ul>
			{{#each section.skills as |skill|}}
				<li>{{bold-first-piece skill ":"}}</li>
			{{/each}}
		</ul>
	</ul>
{{/each}}
