{{timeline-component icon="folder-open"}}
<h2>{{title}}</h2>
<ul>
	{{#each projects as |project|}}
    <div class="project">
    	<a href="{{project.url}}"><img src={{project.screenshot}}/></a><br/>
    	<a href="{{project.url}}">{{project.name}}</a>
    	<p><i>{{project.overview}}</i></p>
   		<p>{{project.contribution}}</p>
    </div>
    {{/each}}
</ul>
