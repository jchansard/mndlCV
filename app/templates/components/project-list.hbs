{{image-gallery}}
{{timeline-component icon="folder-open"}}
<h2>{{title}}</h2>
{{#each projects as |project|}}
	<div class="project row">
    	<div class="col-md-4 outline">
    		<div class="img-container"><a href="{{project.url}}"><img src={{project.screenshot}}/></a></div>
    	</div>
    	<div class="col-md-8">
    		<h4><a class="emphasize bold" href="{{project.url}}">{{project.name}}:</a> <span class="italic">{{project.overview}}</span></h4>
    		<p>{{project.contribution}}</p>
    	</div>
    </div>
{{/each}}