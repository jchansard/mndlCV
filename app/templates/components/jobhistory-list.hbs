{{timeline-component icon="edit"}}
<h2>{{title}}</h2>
<ul class="list-unstyled">
	{{#each jobs as |job|}}
    	<li class="emphasize"><span class="bold">{{job.position}}</span> at {{job.company}} <span class="right">({{job.start}} to {{job.end}})</span></li>
    	<ul>
    		{{#each job.summary as |accomplishment|}}
    			<li>{{accomplishment}}</li>
    		{{/each}}
    	</ul>
    {{/each}}
</ul>
